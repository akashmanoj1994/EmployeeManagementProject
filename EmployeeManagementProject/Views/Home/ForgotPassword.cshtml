
@{
    ViewBag.Title = Resources.PageTitles.ForgotPassword;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br/>
<p class="text-success">@TempData["resetmessage"]</p>
<br />
<h3 class="text-primary">Please enter EmailID</h3>

@using (Html.BeginForm("ForgotPasswordPost", "Home", FormMethod.Post))
{
<br />
<label class="text-info">Email:</label>
@Html.TextBox("emailID")
<br/>
<input type="submit" value="Send link to mail"/>
}


